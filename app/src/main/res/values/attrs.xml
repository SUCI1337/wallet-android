<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="customFont" format="enum">
        <enum name="black" value="0" />
        <enum name="heavy" value="1" />
        <enum name="medium" value="2" />
        <enum name="regular" value="3" />
        <enum name="light" value="4" />
        <enum name="roman" value="5" />
    </attr>

    <declare-styleable name="TariTextView">
        <attr name="customFont" />
    </declare-styleable>
    <declare-styleable name="TariEditText">
        <attr name="customFont" />
    </declare-styleable>
    <declare-styleable name="TariButton">
        <attr name="customFont" />
    </declare-styleable>

</resources>